<?xml version="1.0" encoding="UTF-8"?>
<org.eclipse.emfforms.coffee.model:Machine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:org.eclipse.emfforms.coffee.model="http://www.eclipse.org/emfforms/example/coffeemodel"
    xsi:schemaLocation="http://www.eclipse.org/emfforms/example/coffeemodel Coffee.ecore"
    name="Super Brewer 3000">
  <children
      xsi:type="org.eclipse.emfforms.coffee.model:BrewingUnit">
    <children
        xsi:type="org.eclipse.emfforms.coffee.model:ControlUnit">
      <processor
          vendor="Qualcomm"
          clockSpeed="5"
          numberOfCores="10"
          socketconnectorType="Z51"
          thermalDesignPower="1000"/>
      <display
          width="20"
          height="10"/>
    </children>
    <activities
        name="Preheat"/>
    <activities
        name="Brew"/>
  </children>
</org.eclipse.emfforms.coffee.model:Machine>
